{
  "name": "corgii",
  "version": "1.0.0",
  "description": "find utils components what you need",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build:vue": "pnpm run --filter @corgii/vue build",
    "build:react": "pnpm run --filter @corgii/react build",
    "docs:react": "pnpm run --filter @corgii/docs-react start",
    "docs-build:react": "pnpm run --filter @corgii/docs-react build",
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --no-fix",
    "lint-staged": "lint-staged",
    "test": "pnpm run test:utils && pnpm run test:components",
    "test:utils": "pnpm run --filter @corgii/utils test",
    "test:components": "pnpm run --filter @corgii/react test",
    "prepare": "husky install"
  },
  "keywords": ["components", "vue", "react", "utils", "antd", "element"],
  "author": "apine",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "antd": "^5.1.1",
    "element-plus": "^2.2.27",
    "id3js": "^2.1.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@edge-runtime/vm": "2.0.2",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "cypress": "^12.0.0",
    "dumi": "^2.0.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-vue": "^9.6.0",
    "happy-dom": "^8.0.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "lint-staged": "13.1.0",
    "naive-ui": "^2.33.4",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.26.0",
    "vue-tsc": "^1.0.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint"
    ]
  }
}
