{
  "name": "corgwn",
  "version": "1.0.0",
  "description": "Explore the components you need",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build:vue": "pnpm run --filter @corgwn/vue build",
    "build:react": "pnpm run --filter @corgwn/react build",
    "build:types": "pnpm run --filter @corgwn/types build",
    "build:icons": "pnpm build:icons-react && pnpm build:icons-vue",
    "build:icons-react": "pnpm --filter @pembroke/icons build",
    "build:icons-vue": "pnpm --filter @cardigan/icons build",
    "docs:react": "pnpm run --filter @corgwn/docs-react start",
    "docs-build:react": "pnpm run --filter @corgwn/docs-react build",
    "docs:vue": "pnpm --filter @corgwn/docs-vue docs:dev",
    "docs-build:vue": "pnpm --filter @corgwn/docs-vue docs:build",
    "demo:react": "pnpm run --filter @corgwn/demo-react dev",
    "demo:vue": "pnpm run --filter @corgwn/demo-vue dev",
    "lint": "pnpm prelint && pnpm run lint:eslint",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --no-fix",
    "lint-staged": "lint-staged",
    "prelint": "pnpm build:icons",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:unit": "pnpm run test:utils --run && pnpm run test:hooks --run",
    "test:e2e": "pnpm run test:react",
    "test:utils": "pnpm run --filter @corgwn/utils test",
    "test:hooks": "pnpm run --filter @corgwn/hooks test",
    "test:react": "pnpm run --filter @corgwn/react test",
    "tsc": "pnpm run tsc:react && pnpm run tsc:vue && pnpm run tsc:utils && pnpm run tsc:types && pnpm run tsc:composables && pnpm run tsc:hooks",
    "tsc:react": "pnpm run --filter @corgwn/react tsc",
    "tsc:vue": "pnpm run --filter @corgwn/vue tsc",
    "tsc:utils": "pnpm run --filter @corgwn/utils tsc",
    "tsc:types": "pnpm run --filter @corgwn/types tsc",
    "tsc:composables": "pnpm run --filter @corgwn/composables tsc",
    "tsc:hooks": "pnpm run --filter @corgwn/hooks tsc",
    "prepare": "husky install"
  },
  "keywords": [
    "components",
    "vue",
    "react",
    "utils",
    "antd",
    "element",
    "corgwn"
  ],
  "author": "apine",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^5.0.0",
    "@ant-design/icons-vue": "^7.0.1",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "ant-design-vue": "^4.0.3",
    "antd": "^5.1.1",
    "id3js": "^2.1.1",
    "immer": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "react-transition-group": "^4.4.5",
    "use-immer": "^0.9.0",
    "vue": "^3.3.4",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "4"
  },
  "devDependencies": {
    "@algolia/client-search": ">= 4.9.1 < 6",
    "@babel/core": "^7.19.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-typescript": "^7.22.15",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@edge-runtime/vm": "3.2.0",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/babel__core": "^7.20.3",
    "@types/eslint": "^8.44.6",
    "@types/jsdom": "^21.0.0",
    "@types/lodash-es": "^4.17.10",
    "@types/markdown-it": "^13.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "cypress": "^13.3.2",
    "dumi": "^2.1.10",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.31.9",
    "eslint-plugin-vue": "^9.6.0",
    "happy-dom": "^12.0.0",
    "husky": "^8.0.1",
    "jsdom": "^22.0.0",
    "lint-staged": "15.2.5",
    "lodash-es": "^4.17.21",
    "markdown-it": "^13.0.1",
    "naive-ui": "^2.33.4",
    "postcss": "^8.4.20",
    "prettier": "^3.0.0",
    "react-is": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^4.0.0",
    "styled-components": "^6.0.0",
    "stylelint": "^16.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-svgr": "^4.0.0",
    "vite-svg-loader": "^5.0.0",
    "vitepress": "1.0.0-rc.25",
    "vitest": "^1.0.0",
    "vue-tsc": "^1.8.19",
    "webpack": "^5.75.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint"
    ]
  }
}
