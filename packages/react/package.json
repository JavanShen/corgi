{
  "name": "@corgwn/react",
  "version": "1.0.0-alpha.4",
  "type": "module",
  "description": "Explore the components you need",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "pnpm run test:component",
    "test:dev": "cypress open --component -b edge",
    "test:component": "cypress run --component",
    "tsc": "tsc",
    "prepublishOnly": "pnpm run test && pnpm run build"
  },
  "files": [
    "lib",
    "es"
  ],
  "publishConfig": {
    "main": "./lib/index.js",
    "module": "./es/index.js",
    "types": "./es/react/index.d.ts",
    "access": "public",
    "exports": {
      ".": {
        "import": "./es/index.js",
        "require": "./lib/index.js"
      }
    }
  },
  "keywords": [
    "react",
    "antd",
    "components",
    "corgwn"
  ],
  "dependencies": {
    "@corgwn/hooks": "workspace:1.0.0-alpha.2",
    "@corgwn/types": "workspace:^1.0.1",
    "@corgwn/utils": "workspace:1.0.0-alpha.3",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@pembroke/icons": "workspace:^1.0.0",
    "react-transition-group": "^4.4.5"
  },
  "peerDependencies": {
    "antd": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": ["react", "react-dom", "@babel/core"]
    }
  }
}